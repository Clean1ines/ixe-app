# Application Environment
ENVIRONMENT=prod
LOG_LEVEL=WARNING

# Database Configuration
DATABASE_URL=sqlite:///./ege_problems.db
DATABASE_ECHO=false
DATABASE_POOL_SIZE=20
DATABASE_MAX_OVERFLOW=30

# Browser Configuration
BROWSER_TIMEOUT=30
BROWSER_HEADLESS=true
BROWSER_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36
BROWSER_VIEWPORT_WIDTH=1920
BROWSER_VIEWPORT_HEIGHT=1080
BROWSER_BASE_URL=https://ege.fipi.ru

# Scraping Configuration
SCRAPING_BASE_URL=https://fipi.ru
SCRAPING_MODE=sequential
SCRAPING_MAX_EMPTY_PAGES=2
SCRAPING_MAX_PAGES=
SCRAPING_FORCE_RESTART=false
SCRAPING_PARALLEL_WORKERS=3
SCRAPING_RETRY_ATTEMPTS=3
SCRAPING_RETRY_DELAY=1

# Assets Configuration
ASSETS_DIRECTORY=./assets
ASSET_DOWNLOAD_TIMEOUT=60
MAX_CONCURRENT_DOWNLOADS=5

# Graceful Degradation
ENABLE_GRACEFUL_DEGRADATION=true
DEFAULT_TIMEOUT=30
DEFAULT_RETRIES=2
